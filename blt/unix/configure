if test -r $TKINCDIR/tk.h; then
  blt_path_tk_h=$TKINCDIR
fi

TKINCSW=""
TKINCDIR="${blt_path_tk_h}"
if test -z "${blt_path_tk_h}" ; then
    TKINCDIR="__Edit_${saveFile}__"
    echo "$ac_t""no" 1>&6
else
    echo "$ac_t""${blt_path_tk_h}" 1>&6
fi

if test "$TKINCDIR" != "/usr/include" ; then
    TKINCSW="-I$TKINCDIR"
fi





