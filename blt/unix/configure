#include <float.h>
#endif
main () {
#ifndef DBL_EPSILON 
    double eps, u, comp;
    u = 1.0;
    for(;;) {
       u *= 0.5;
       comp = 1.0 + u;
       if (comp == 1.0) {
	   break;
       }
